    <content>&lt;p dir="ltr" style="text-align: left;"&gt;&lt;/p&gt;&lt;div&gt;&lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{d0f2af72-d7d5-4063-bfe2-9d81a1f37b68}{234}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:10,&amp;quot;335559731&amp;quot;:0,&amp;quot;335559737&amp;quot;:0,&amp;quot;335562764&amp;quot;:2,&amp;quot;335562765&amp;quot;:1.14,&amp;quot;335562766&amp;quot;:4,&amp;quot;335562767&amp;quot;:0,&amp;quot;335562768&amp;quot;:4,&amp;quot;335562769&amp;quot;:6}"&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;Take a minute and write down your response to the following&amp;nbsp;&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;three questions:&amp;nbsp;&lt;/span&gt;​&lt;/p&gt;&lt;/div&gt;&lt;ul role="list"&gt;&lt;li data-margin="730" data-aria-posinset="1" role="listitem" data-aria-level="1"&gt;&lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{d0f2af72-d7d5-4063-bfe2-9d81a1f37b68}{237}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:730,&amp;quot;335559731&amp;quot;:-237,&amp;quot;335559737&amp;quot;:0,&amp;quot;335562764&amp;quot;:2,&amp;quot;335562765&amp;quot;:1.14,&amp;quot;335562766&amp;quot;:4,&amp;quot;335562767&amp;quot;:12,&amp;quot;335562768&amp;quot;:4,&amp;quot;335562769&amp;quot;:6}" data-indent="-237"&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;I am a good teacher because…&amp;nbsp;&lt;/span&gt;​&lt;/p&gt;&lt;/li&gt;&lt;li data-margin="730" data-aria-posinset="2" role="listitem" data-aria-level="1"&gt;&lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{d0f2af72-d7d5-4063-bfe2-9d81a1f37b68}{240}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559